class Solution {
public:
    string reverseWords(string s) {
        stringstream ss(s);
        vector<string> vec;
        string tmp;
      while (ss >> tmp) {
            vec.push_back(tmp);
        }
      string ans;
    for (int i = vec.size() - 1; i >= 0; i--) {
            ans += vec[i];
            if (i != 0) ans += " ";
        }

        return ans;
    }
};
